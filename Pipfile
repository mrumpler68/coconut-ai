[[source]]
url = "https://pypi.org/simple"
verify_ssl = true
name = "pypi"

[requires]
python_version = "3.10"

[scripts]
lint-editorconfig = "ec"
lint-python = "autopep8 src"
lint-types = "mypy src"
start = "python -m src"
build = "python setup.py sdist"

[dev-packages]
autopep8 = "==2.0.4"
editorconfig-checker = "==2.7.3"
mypy = "==1.7.0"
setuptools = "*"
